    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.2.1",
    "@web/dev-server": "^0.1.18",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
